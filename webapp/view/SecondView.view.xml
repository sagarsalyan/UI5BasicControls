<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:dnd="sap.ui.core.dnd" xmlns="sap.m"
	xmlns:ui="sap.ui.table" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	controllerName="BasicControls.BasicControls.controller.SecondView" xmlns:html="http://www.w3.org/1999/xhtml">
	<App>
		<pages>
			<Page title="Title">
				<content>
					<VBox>
						<ui:Table id="idResultsTable" visibleRowCountMode="Auto"></ui:Table>
						<HBox>
							<Button text="DownloadCSV" press="onDownloadCSV"/>
							<Button text="DownloadSpreedSheet" press="onDownloadSS"/>
						</HBox>
						<Table id="idResultsTablem" selectionChange="onSelectionChange" mode="MultiSelect" sticky="HeaderToolbar,ColumnHeaders"></Table>
						<HBox>
							<Button text="Send" press="onSend"/>
							<Button text="Total Mark" press="onTotal"/>
						</HBox>
						<!--Table for row addition-->
						<Table id="mathTable" headerText="Mathematical Operation" sticky="ColumnHeaders" items="{oRows>/}" popinLayout="Block">
							<columns>
								<Column>
									<Text text="Value1"/>
								</Column>
								<Column>
									<Text text="Operation"/>
								</Column>
								<Column>
									<Text text="Value2"/>
								</Column>
								<Column>
									<Text text="Result"/>
								</Column>
								<Column>
									<Text text="Action"/>
								</Column>
							</columns>
							<items>
								<ColumnListItem>
									<cells>
										<Input liveChange="onLiveChange"/>
										<Text text="{oRows>exp}"/>
										<Input liveChange="onLiveChange"/>
										<Input/>
										<Button text="Clear" press="onClear"/>
									</cells>
								</ColumnListItem>
								<!--<ColumnListItem>-->
